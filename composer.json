{
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "craftcms/ckeditor": "4.3.1",
    "craftcms/cms": "5.5.x-dev as 5.5.0-alpha",
    "doublesecretagency/craft-googlemaps": "5.0.2",
    "ether/simplemap": "5.0.3",
    "verbb/comments": "3.0.3",
    "vlucas/phpdotenv": "^5.4.0"
  },
  "require-dev": {
    "craftcms/generator": "^2.0.0",
    "craftcms/wp-import": "dev-main",
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "config": {
    "allow-plugins": {
      "craftcms/plugin-installer": true,
      "yiisoft/yii2-composer": true
    },
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "8.2"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example.dev', '.env');\""
    ]
  },
  "repositories": {
    "wp-import": {
      "type": "vcs",
      "url": "https://github.com/craftcms/wp-import"
    },
    "0": {
      "type": "composer",
      "url": "https://composer.craftcms.com",
      "canonical": false
    }
  }
}
